  right: 10px;
  top: 0;
  margin: 9px 0 0 0;
  border: 0;
  z-index: 1;
  outline: none;
  width: 16px;
  height: 16px;
  background-color: transparent;
  background-image: url("../images/icon_search.png");
  background-size: cover;
}
.company_search_bx .icon.reset {
  background-image: url("../images/icon_reset.png");
}

.company_list_select {
  display: none;
  position: absolute;
  left: 0;
  top: 32px;
  min-width: 170px;
  border-radius: 4px;
  border: solid 1px #ededed;
  background-color: #ffffff;
  max-height: 320px;
  overflow: hidden;
  overflow-y: auto;
  z-index: 10;
}
.company_list_select.on {
  display: block;
}
.company_list_select li {
  border-top: 1px solid #ededed;
}
.company_list_select li:first-child {
  border-top: 0;
}
.company_list_select li input[type="radio"],
.company_list_select li input[type="checkbox"] {
  position: absolute;
  left: 0;
  top: 0;
  height: 1;
  width: 1;
  visibility: hidden;
}
.company_list_select li label {
  position: relative;
  display: block;
  width: 100%;
  line-height: 1.5;
  color: #222222;
  padding: 6px 10px;
  background: #fff;
  overflow: hidden;
  cursor: pointer;
}
.company_list_select li input + label {
  position: relative;
  white-space: nowrap;
  word-break: break-all;
  word-wrap: break-word;
}
.company_list_select li input[type="radio"]:checked + label,
.company_list_select li input[type="checkbox"]:checked + label {
  border-radius: 4px;
  background-color: #f0f9ff;
  color: #222222;
}

/* Chrome, Safari용 스크롤 바 */
.scrollCustom::-webkit-scrollbar {
  width: 15px;
  height: 5px;
  /*border: 3px solid #fff;*/
}

.scrollCustom::-webkit-scrollbar-button:start:decrement,
.scrollCustom::-webkit-scrollbar-button:end:increment {
  display: none;
  height: 10px;
  /* background: url('./images/bg.png') #efefef; */
}
.scrollCustom::-webkit-scrollbar-track {
  /*background: #efefef;*/
  -webkit-border-radius: 5px;
  border-radius: 5px;
  /*-webkit-box-shadow: inset 0 0 4px rgba(0, 0, 0, 0.05);*/
}
.scrollCustom::-webkit-scrollbar-thumb {
  height: 50px;
  width: 50px;
  background: rgba(0, 0, 0, 0.1);
  -webkit-border-radius: 5px;
  border-radius: 5px;
  /*-webkit-box-shadow: inset 0 0 4px rgba(0, 0, 0, 0.1);*/
}

.range-slider {
  position: relative;
  margin: 5px 0 0 0;
}

.range-slider {
  width: 100%;
}

.range-slider__range {
  -webkit-appearance: none;
  width: calc(100% - (120px));
  height: 10px;
  border-radius: 5px;
  background: #d7dcdf;
  outline: none;
  padding: 0;
  margin: 0;
}
.range-slider__range::-webkit-slider-thumb {
  -webkit-appearance: none;
  appearance: none;
  width: 20px;
  height: 20px;
  border-radius: 50%;
  background: #2c3e50;
  cursor: pointer;
  -webkit-transition: background 0.15s ease-in-out;
  transition: background 0.15s ease-in-out;
}
.range-slider__range::-webkit-slider-thumb:hover {
  background: #1abc9c;
}
.range-slider__range:active::-webkit-slider-thumb {
  background: #1abc9c;
}
.range-slider__range::-moz-range-thumb {
  width: 20px;
  height: 20px;
  border: 0;
  border-radius: 50%;
  background: #2c3e50;
  cursor: pointer;
  -moz-transition: background 0.15s ease-in-out;
  transition: background 0.15s ease-in-out;
}
.range-slider__range::-moz-range-thumb:hover {
  background: #1abc9c;
}
.range-slider__range:active::-moz-range-thumb {
  background: #1abc9c;
}
.range-slider__range:focus::-webkit-slider-thumb {
  box-shadow: 0 0 0 3px #fff, 0 0 0 6px #1abc9c;
}

.range-slider__value {
  display: inline-block;
  position: relative;
  width: 95px;
  color: #fff;
  line-height: 20px;
  text-align: center;
  border-radius: 4px;
  background: #677ad4;
  padding: 5px 10px;
  margin-left: 20px;
}

.range-slider__total {
  display: inline-block;
  position: relative;
  width: 100%;
  color: #677ad4;
  font-size: 16px;
  text-align: right;
  padding: 5px 10px;
  margin-top: 20px;
  margin-right: 33px;
}

.range-slider__value:after {
  position: absolute;
  top: 8px;
  left: -7px;
  width: 0;
  height: 0;
  border-top: 7px solid transparent;
  border-right: 7px solid #677ad4;
  border-bottom: 7px solid transparent;
  content: "";
}

.range-slider ::-moz-range-track {
  background: #d7dcdf;
  border: 0;
}

.range-slider input::-moz-focus-inner,
.range-slider input::-moz-focus-outer {
  border: 0;
}
.range-slider .per {
  position: absolute;
  right: 8px;
  top: 9px;
  color: #fff;
  font-size: 12px;
}

