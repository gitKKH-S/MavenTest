.select_sty {
  position: relative;
  display: inline-block;
  width: 170px;
  margin: 0 3px 5px 0;
  vertical-align: middle;
}
.select_sty.middle {
  width: 126px;
}
.select_sty.step01 {
  width: 258px;
}
.select_sty.step02 {
  width: 170px;
}
.select_sty select {
  display: inline-block;
  width: 100%;
  height: 32px;
  padding: 6px 30px 8px 15px;
  outline: 0;
  border: 1px solid #aaaaaa;
  border-radius: 4px;
  background: #fff;
  color: #333;
  font-size: 12px;
  text-align-last: center;
  text-align: center;
  appearance: none;
  -webkit-appearance: none;
  -moz-appearance: none;
  cursor: pointer;
}
.select_sty select::-ms-expand {
  display: none;
}
.select_sty select:hover {
  color: #333;
  background: #eee;
  border: 1px solid #eee;
}
.select_sty select:disabled {
  opacity: 0.5;
  pointer-events: none;
}
.select_sty i {
  position: absolute;
  top: 10px;
  right: 10px;
  display: inline-block;
  width: 12px;
  height: 12px;
  pointer-events: none;
  background-image: url("../images/think_menu_arrow_down.png");
  background-repeat: no-repeat;
  background-size: cover;
}
.select_sty select:hover ~ i {
  background-image: url("../images/think_menu_arrow_down.png");
  animation: flip-horizontal-bottom 0.4s cubic-bezier(0.455, 0.03, 0.515, 0.955)
    both;
  -webkit-animation: flip-horizontal-bottom 0.4s
    cubic-bezier(0.455, 0.03, 0.515, 0.955) both;
}
@keyframes flip-horizontal-bottom {
  0% {
    transform: rotateX(0);
  }
  50% {
    transform: rotateX(90deg);
  }
  100% {
    transform: rotateX(180deg);
  }
}

